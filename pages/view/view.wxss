}
.tab_0_btn{
  z-index: -1;
  margin-top: 30rpx;
}
.user_head{
  height: 100rpx;
  width: 100rpx;
  border-radius: 50%
}
.user_msg_list{
  margin: 30rpx;
}
.user_name{
  font-size:38rpx;
  color: #F4BE26;
  margin-left: 15rpx;
}
.user_time{
  font-size: 28rpx;
  margin-left: 15rpx;
  color: #666
}
.user_msg_title{
  margin-top: 20rpx;
  margin-bottom: 20rpx
}
.user_msg_centent_arr{
  color: #15BBF9;
  font-size: 32rpx;
}
.user_msg_icon{
  float: right;
  width: 260rpx;
  height: 60rpx;
}
.user_msg_icon image{
  height: 60rpx;
}
.user_msg_centent{
  font-size: 32rpx;
  color: #666;
    word-break:break-all;
  white-space: pre-wrap;
  overflow:hidden;
  text-overflow:ellipsis;
  display:-webkit-box;
  -webkit-line-clamp:4;
  -webkit-box-orient:vertical;
}
.view_outers{
    display: flex;
  padding-top: 10rpx;
  flex-direction: row;
  flex-wrap:wrap;
}
.view_li{
  padding: 5rpx;
}
.tab3_lunbo button{
  height: 65rpx;
  width: 20%;
  line-height: 65rpx;
  margin: 5rpx;
  z-index: -1;
  padding: 5rpx;
}
.page_row {
  display: flex;
  flex-direction: row;
} 
.post_message_input{
  margin: 5rpx;
  width: 75%;
  border: 1px solid #d0d0d0;
  border-radius: 15rpx;
  padding: 5rpx;
}
.swiper-tab-list {
  font-size: 29rpx;
  display: inline-block;
  width: 49%;
  color: #333;
}

.swiper-tab {
  height: 82rpx;
  width: 100%;
  border-bottom: 2rpx solid #eee;
  text-align: center;
  line-height: 80rpx;
  background: #fff;
  font-size: 14px;
}

.on {
  color: #dd2727;
  border-bottom: 5rpx solid #dd2727;
}

.bre {
  border-right: 1px solid #eee;
}

.Content {
  padding: 10px;
  border-radius: 6%;
}

.Content .tip1 {
  margin-bottom: 15px;
}

.Content .tip2 {
  margin-top: 15px;
}

.Content .link {
  color: #5587fd;
}

.scroll-view_H {
  white-space: nowrap;
  width: 100%;
  background-color: rgba(255, 255, 255, 0.7);
}

.banner-scroll {
  margin-top: 2px;
}

.banner-scroll repeat {
  margin-bottom: 10px;
}

.nav-name {
  display: inline-block;
  margin: 0 5px;
  font-size: 16px;
  color: #2b2e33;
  border-bottom: 2px solid transparent;
  padding: 10px;
}

.nav-hover {
  color: #f06000;
  border-bottom: 2px solid #f06000;
}

.wxParse-p image {
  text-align: center;
  margin: 5rpx auto;
  display: block;
  max-width: 100%;
}

.praise_btn_line {
  margin: 15px auto 0;
  display: block;
}

.digbg {
  background-image: url("data:image/png;base64,");
  width: 52px;
  height: 22px;
  float: left;
}

.praise_text {
  float: right;
  width: 29px;
  height: 22px;
  color: #fff;
  text-align: center;
  cursor: pointer;
}

.praise_data {
  float: left;
  position: relative;
  height: 20px;
  padding: 0 7px;
  margin-left: 7px;
  border: 1px solid #ecd181;
  border-radius: 2px;
  background: #fcf8e9;
  background: -webkit-linear-gradient(top, #fef7df, #f9f9f6);
  background: -o-linear-gradient(top, #fef7df, #f9f9f6);
  background: -moz-linear-gradient(top, #fef7df, #f9f9f6);
  vertical-align: middle;
  line-height: 20px;
  font-size: 12px !important;
  overflow: visible;
}

.data_arrow {
  background-image: url("data:image/png;base64,");
  position: absolute;
  width: 6px;
  height: 7px;
  background-position: 0 -75px;
  left: -6px;
  top: 6px;
}

.video video {
  width: 100%;
}
